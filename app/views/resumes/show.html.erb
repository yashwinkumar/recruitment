<% @resume_sections.each do |resume_sec| %>
  <div class="row">
      <div class="thumbnail">
        <h3><%= resume_sec.section.name %></h3>
        <%= video_tag resume_sec.video, :size => "280x240", :controls => true %>
        <div class="caption">

          <p>Rating: <%= resume_sec.rating %></p>
        </div>
      </div>
  </div>
<% end %>
